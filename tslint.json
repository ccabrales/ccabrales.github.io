{
  "rulesDirectory": ["tslint-plugin-prettier"],
  "extends": ["tslint:latest", "tslint-react", "tslint-config-prettier"],
  "rules" : {
    "interface-name": false,
    "jsx-no-lambda": false,
    "jsx-no-multiline-js": false,
    "prettier": true
  },
  "linterOptions": {
    "exclude": ["*.js"]
  }
}
