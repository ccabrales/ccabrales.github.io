{
  "name": "ccabrales.github.io",
  "private": true,
  "description": "Casey Cabrales Portfolio",
  "homepage": "https://ccabrales.github.io",
  "version": "1.0.0",
  "author": "Casey Cabrales",
  "dependencies": {
    "gatsby": "^2.19.45",
    "gatsby-image": "^2.2.44",
    "gatsby-plugin-offline": "^3.0.41",
    "gatsby-plugin-react-helmet": "^3.1.24",
    "gatsby-plugin-sharp": "^2.4.13",
    "gatsby-source-filesystem": "^2.1.56",
    "gatsby-transformer-sharp": "^2.3.19",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-helmet": "^5.2.1"
  },
  "devDependencies": {
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@types/react-helmet": "^5.0.15",
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "@typescript-eslint/parser": "^2.24.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-config-react-app": "^5.2.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "gatsby-plugin-eslint": "^2.0.8",
    "gatsby-plugin-sass": "^2.1.31",
    "gatsby-plugin-typescript": "^2.2.5",
    "gh-pages": "^2.2.0",
    "node-sass": "^4.13.1",
    "prettier": "^1.19.1",
    "typescript": "^3.8.3"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "deploy": "gatsby build && gh-pages -d public -b master",
    "develop": "gatsby develop",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx}",
    "lint": "eslint --ext ts,tsx,js,jsx src/",
    "lint:fix": "yarn lint --fix",
    "prebuild": "yarn verify",
    "predeploy": "yarn verify",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\"",
    "type-check": "tsc --noEmit",
    "verify": "yarn type-check && yarn test && yarn lint"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ccabrales/ccabrales.github.io"
  },
  "bugs": {
    "url": "https://github.com/ccabrales/ccabrales.github.io/issues"
  }
}
